{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "create-migration": "node src/db/create-migration.js",
    "migrate": "ts-node ./src/db/migrations/migrate.ts",
    "create-seed": "node src/db/create-seed.js",
    "seed": "npx ts-node src/db/seeds/seed.ts",
    "check-types": "tsc --noEmit",
    "build": "tsc && tsc-alias && cpx \"src/config/systemPrompt.md\" dist/config",
    "start": "node dist/index.js"
  },
